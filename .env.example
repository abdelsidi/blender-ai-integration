# Blender AI Integration - Environment Variables
# انسخ هذا الملف إلى .env وعدل القيم

# OpenAI API Key (لـ DALL-E)
OPENAI_API_KEY=your_openai_api_key_here

# Stability AI API Key (لـ Stable Diffusion)
STABILITY_API_KEY=your_stability_api_key_here

# Replicate API Key (لـ Midjourney)
REPLICATE_API_TOKEN=your_replicate_token_here

# إعدادات اختيارية
BLENDER_VERSION=3.0
DEBUG=false
CACHE_ENABLED=true
